{
    "name": "barn2/woocommerce-product-page-shipping-calculator",
    "description": "WooCommerce Product Page Shipping Calculator",
    "type": "wordpress-plugin",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Barn2 Plugins",
            "email": "support@barn2.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "wp-coding-standards/wpcs": "^2.3",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.0"
    },
    "scripts": {
		"no-dev": "composer update --prefer-dist --no-dev"
    },
    "autoload": {
        "psr-4": {
            "Barn2\\Plugin\\WooCommerce_Product_Page_Shipping_Calculator\\": "src",
            "Barn2\\WPPSC_Lib\\": "lib"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
